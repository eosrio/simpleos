{
  "name": "simpleos",
  "version": "1.0.3",
  "productName": "simpleos",
  "appId": "io.eosrio.simpleos",
  "compilerVersion": "DEFAULT",
  "description": "EOSIO Blockchain Interface & Wallet",
  "author": {
    "name": "EOS Rio",
    "email": "contact@eosrio.io"
  },
  "repository": "https://github.com/eosrio/simpleos",
  "license": "MIT",
  "scripts": {
    "config:default": "node configurations/build-simpleos.js",
    "config:liberland": "node configurations/build-liberland.js",
    "electron:start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "electron:serve": "wait-on http-get://localhost:7777/ && electron index.js --serve --devtools",
    "start:electron-dev": "electron index.js --devtools --debug",
    "start:electron-prod": "electron index.js",
    "start:electron-prod-automode": "electron index.js --autostart",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:mac": "electron-builder build --mac --publish=never",
    "build:win": "electron-builder build --win --publish=never",
    "build:linux": "electron-builder build --linux --publish=never",
    "build:linux-snap": "electron-builder build --linux snap --publish=never",
    "ng": "ng",
    "ng:serve": "ng serve --port 7777",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "release:default": "npm run config:default && npm run build:prod && electron-builder",
    "release:liberland": "npm run config:liberland && npm run build:prod && electron-builder",
    "postinstall": "electron-builder install-app-deps && npm run postinstall:electron",
    "postinstall:electron": "node scripts/postinstall.js"
  },
  "engines": {
    "node": ">= 13.0.0",
    "npm": ">= 6.0.0"
  },
  "private": true,
  "main": "index.js",
  "build": {
    "appId": "simpleos",
    "directories": {
      "output": "dist",
      "buildResources": "build"
    },
    "files": [
      "index.js",
      "ng-dist",
      "static",
      "electron_modules",
      "build/icons/**/*"
    ],
    "afterSign": "electron-builder-notarize",
    "mac": {
      "hardenedRuntime": true,
      "entitlements": "scripts/entitlements.mac.plist",
      "category": "public.app-category.finance",
      "target": [
        "dmg"
      ],
      "icon": "icons/simpleos/icon.icns"
    },
    "dmg": {
      "sign": false
    },
    "linux": {
      "executableName": "simpleos",
      "target": [
        "appImage",
        "deb"
      ],
      "icon": "icons/simpleos",
      "category": "Network",
      "desktop": {
        "Name": "SimplEOS",
        "GenericName": "SimplEOS Wallet",
        "X-GNOME-FullName": "simpleos",
        "Comment": "SimplEOS Blockchain Wallet",
        "Type": "Application",
        "Terminal": "false",
        "StartupNotify": "false",
        "Categories": "Network;"
      }
    },
    "snap": {
      "confinement": "classic",
      "grade": "stable"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "icons/simpleos/icon.ico"
    },
    "appx": {
      "backgroundColor": "#232526",
      "languages": [
        "en-US"
      ],
      "showNameOnTiles": true,
      "identityName": "EOSRio.SimplEOS"
    },
    "nsis": {
      "oneClick": true,
      "createDesktopShortcut": true
    },
    "protocols": [
      {
        "name": "simpleos",
        "role": "Viewer",
        "schemes": [
          "simpleos"
        ]
      }
    ]
  },
  "files": [
    "*.js",
    "build",
    "static",
    "ng-dist",
    "node_modules"
  ],
  "dependencies": {
    "@ajsf/core": "0.1.3",
    "@ajsf/material": "0.1.3",
    "@angular/animations": "9.0.2",
    "@angular/cdk": "9.0.1",
    "@angular/common": "9.0.2",
    "@angular/core": "9.0.2",
    "@angular/flex-layout": "9.0.0-beta.29",
    "@angular/forms": "9.0.2",
    "@angular/material": "9.0.1",
    "@angular/material-moment-adapter": "9.0.1",
    "@angular/platform-browser": "9.0.2",
    "@angular/platform-browser-dynamic": "9.0.2",
    "@angular/router": "9.0.2",
    "@clr/angular": "3.0.0-rc.1",
    "@clr/core": "3.0.0-rc.1",
    "@clr/icons": "3.0.0-rc.1",
    "@clr/ui": "3.0.0-rc.1",
    "@fortawesome/angular-fontawesome": "^0.6.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/pro-duotone-svg-icons": "^5.12.1",
    "@fortawesome/pro-light-svg-icons": "^5.12.1",
    "@fortawesome/pro-regular-svg-icons": "^5.12.1",
    "@fortawesome/pro-solid-svg-icons": "^5.12.1",
    "@ledgerhq/hw-transport": "^5.9.0",
    "@ledgerhq/hw-transport-node-hid": "^5.9.0",
    "ajv": "^6.11.0",
    "angular2-text-mask": "9.0.0",
    "angular2-toaster": "8.0.0",
    "animate.css": "^3.7.2",
    "asn1-ber": "^1.0.9",
    "auto-launch": "^5.0.5",
    "bcrypt": "^3.0.8",
    "bip32-path": "^0.4.2",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "chart.js": "^2.9.3",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "echarts": "^4.6.0",
    "electron-context-menu": "^0.16.0",
    "eosjs": "^21.0.1-rc2",
    "express": "^4.17.1",
    "fcbuffer": "^2.2.2",
    "fuse.js": "^3.4.6",
    "hammerjs": "^2.0.8",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.4.1",
    "keytar": "^5.2.0",
    "lodash-es": "^4.17.15",
    "lodash.filter": "4.6.0",
    "lodash.get": "^4.4.2",
    "lodash.map": "4.6.0",
    "lodash.set": "4.3.2",
    "lottie-web": "^5.6.4",
    "menu": "^0.2.5",
    "moment": "^2.24.0",
    "ngx-echarts": "^4.2.2",
    "ngx-electron": "^2.2.0",
    "ngx-json-viewer": "^2.4.0",
    "ngx-lottie": "^5.3.2",
    "ngx-markdown": "^9.0.0",
    "ngx-order-pipe": "^2.0.4",
    "ngx-pagination": "^5.0.0",
    "node-fetch": "^2.6.0",
    "node-pre-gyp": "^0.14.0",
    "node-sass": "^4.13.1",
    "node-schedule": "^1.3.2",
    "open": "^7.0.2",
    "popper.js": "^1.16.1",
    "portfinder": "^1.0.25",
    "primeicons": "^2.0.0",
    "primeng": "^9.0.0-rc.4",
    "quill": "^2.0.0-dev.3",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "sudo-prompt": "^9.1.1",
    "tar": "^6.0.1",
    "text-mask-addons": "3.8.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.900.3",
    "@angular/cli": "9.0.3",
    "@angular/compiler": "9.0.2",
    "@angular/compiler-cli": "9.0.2",
    "@angular/language-service": "9.0.2",
    "@electron/typescript-definitions": "8.6.6",
    "@types/crypto-js": "3.1.43",
    "@types/echarts": "4.4.3",
    "@types/express": "4.17.2",
    "@types/node": "13.7.4",
    "@webcomponents/custom-elements": "^1.3.2",
    "codelyzer": "^5.1.2",
    "core-js": "^3.6.4",
    "electron": "^8.0.3",
    "electron-builder": "^22.3.6",
    "electron-builder-notarize": "^1.1.2",
    "electron-reload": "^1.5.0",
    "eslint": "^6.8.0",
    "npm-run-all": "^4.1.5",
    "protractor": "^5.4.3",
    "rxjs": "^6.5.4",
    "rxjs-compat": "^6.5.4",
    "text-encoding-shim": "^1.0.5",
    "ts-node": "^8.6.2",
    "typescript": "3.7.5",
    "wait-on": "^4.0.0-rc.1",
    "webpack": "^4.41.6",
    "webpack-dev-server": "^3.10.3",
    "zone.js": "^0.10.2"
  }
}
